    "test:unit": "jest --config tests/jest.unit.config.js",
    "test:integration": "jest --config tests/jest.integration.config.js",
    "test:watch": "jest --watch --no-cache",
    "eslint:fix": "eslint . --fix --ignore-path .gitignore"
  },
  "engines": {
    "node": "^10.16.0",
    "npm": ">6"
  },
  "dependencies": {
