    "eslint-loader": "^3.0.2",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-graphql": "^3.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
