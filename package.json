    "npm": "^6.13.0",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "styled-components": "^4.4.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
